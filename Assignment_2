import os # already present in the environment therefore no need to install it
import boto3
# Create a new S3 client
s3 = boto3.client('s3') # library.client('s3')
# Upload a file to an S3 bucket
bucket_name = 'parul-binf5506'
s3.upload_file('Assignment_2.py', 'parul-binf5506', 'Assignment_2.py') # file_name, bucket_name, key_name
print(f"File hello.py uploaded to bucket {bucket_name} successfully.")
